
public class MissingElement {
public int[] ARRAY= {1,2,4,5,6,7,10};

public int findMin(int[])
{
	for(int i=0;i<100;i++)
	{
	 if(ARRAY[i] != 0)
	     XOR ^= ARRAY[i];
	 XOR ^= (i + 1);
	}
	return XOR;
}
